#!/bin/sh
#SOURCE: https://salsa.debian.org/installer-team/base-installer/-/raw/master/pkgdetails.c
#HASH: 6f5417b2696b3a74dcc7884694f4fcc14d0285fb

set -e
# set -x

build()
{
  if [ "x$1" = "xx86_64" ]
  then
    gcc -O3 -Wall -o pkgdetails pkgdetails.c
  elif [ "x$1" = "xarm64" ]
  then
    aarch64-linux-gnu-gcc -O3 -Wall -o pkgdetails pkgdetails.c
  fi
}

install()
{
  mkdir -p $1/usr/lib/debootstrap
  cp -f pkgdetails $1/usr/lib/debootstrap
}

binaries()
{
  ls pkgdetails
}
